{
    "schema_name": "pinterest",
    "url_template": true,
    "db": {
        "table_needed": false
    },
    "url_scheme": {
        "tld": ["com", "it", "de"],
        "sld": ["pinterest", "pin"],
        "subd": ["www", ""]
    },
    "categories":
    {
        "available": false
    },
    "subscription": {
        "available": true,
        "subscription_name_locator": 1,
        "url_blueprint": "{scheme}://{subd}.{sld}.{tld}/{subscription_name}"
    },
    "header": 
    {
        "Cookie": "accessAgeDisclaimerPH=1"
    },
    "storage":
    {
        "category_storage": false,
        "base_path": "pinterest"
    }
}